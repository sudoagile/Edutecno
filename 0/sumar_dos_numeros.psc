Algoritmo sumar_dos_numeros
	Escribir "Bienvenido al programa para sumar dos números";
	Definir numero1, numero2, suma Como Entero;
	Escribir "Introduce el número 1";
	Leer numero1;
	Escribir "Introduce el número 2";
	Leer numero2;
	suma <- numero1 + numero2;
	Escribir "El resultado de la suma es: ", suma;
	Escribir "Programa finalizado.";
FinAlgoritmo
